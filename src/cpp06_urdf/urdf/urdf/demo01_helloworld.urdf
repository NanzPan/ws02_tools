<!-- 
    创建一个长方体机器人
        长 1.0 ，宽 0.5 ， 高 0.1
 -->
 <!-- 开头设置名字,根标签 -->
 <robot name="helloworld">
    <!-- 然后设置link连杆 -->
    <link name="base_link">
        <!-- 可视化 -->
        <visual>
            <!-- 几何外观 -->
            <geometry>
                <!-- box就是长方体 -->
                <box size="1.0 0.5 0.1"/>
            </geometry>
        </visual>
    </link>
 </robot>